  "description": "Marketing interview test for back-end",
  "main": "",
  "repository": "https://github.com/JSainsburyPLC/marketing-interview-backend.git",
  "license": "",
  "private": true,
  "dependencies": {
    "@types/aws-lambda": "8.10.39",
    "@types/jest": "24.0.25",
    "@types/node": "13.1.6",
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "aws-lambda": "1.0.5",
    "aws-sdk": "2.602.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-import-resolver-ts": "0.4.2",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "serverless": "1.61.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "5.12.1",
    "serverless-webpack": "5.3.1",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.4",
    "webpack": "4.41.5"
  },
  "scripts": {
    "build": "serverless package --stage local --env local",
    "start": "serverless offline --port 3000 --stage local --env local",
    "test": "jest --watch",
    "test:ci": "CI=true FORCE_COLOR=true jest --runInBand",
    "clean": "yarn exec rimraf ./coverage ./.webpack ./.serverless ./node_modules",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "lint:fix": "yarn lint --fix",
    "prettify": "prettier --list-different './src/**/*.{js,jsx,json,yml,yaml,css,less,scss,ts,tsx,md,graphql,mdx}' ./package.json",
    "prettify:fix": "yarn prettify --write",
    "validate": "yarn lint && yarn prettify"
  }
}
